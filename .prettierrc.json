{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "importOrder": ["^react$", "^@your-package", "^[./]"],
        "importOrderSeparation": true
      }
    }
  ]
}
